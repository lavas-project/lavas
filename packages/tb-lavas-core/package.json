{
  "name": "tb-lavas-core",
  "version": "1.0.2-rc.2",
  "description": "Lavas core implemented by Vue",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava --verbose",
    "prepublish": "babel core --out-dir dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lavas-project/lavas.git"
  },
  "keywords": [
    "lavas",
    "pwa",
    "vue"
  ],
  "author": "sfe-sy <sfe-sy@baidu.com>",
  "license": "ISC",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "ava": {
    "serial": false,
    "failFast": true,
    "concurrency": 10,
    "files": [
      "test/unit/**/*.test.js",
      "test/integration/*.js"
    ],
    "backupFiles": [
      "test/unit/**/*.test.js",
      "test/integration/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "babel-loader": "^7.1.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "graceful-fs": "^4.1.11",
    "hash-sum": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "lodash.template": "^4.4.0",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "path-to-regexp": "^2.1.0",
    "querystring": "^0.2.0",
    "serialize-javascript": "^1.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "super-json": "^2.2.0",
    "sw-register-webpack-plugin": "^1.0.20",
    "url-loader": "^0.5.9",
    "vue": "^2.5.2",
    "vue-loader": "13.0.2",
    "vue-router": "^2.7.0",
    "vue-skeleton-webpack-plugin": "^1.1.11",
    "vue-template-compiler": "^2.5.2",
    "vuex": "^2.3.1",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "workbox-webpack-plugin": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-register": "^6.23.0",
    "chokidar": "^1.7.0",
    "compose-middleware": "^3.0.0",
    "connect-history-api-fallback": "^1.3.0",
    "eventsource-polyfill": "^0.9.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "nyc": "^10.3.2",
    "ora": "^1.3.0",
    "serve-favicon": "^2.4.3",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.0"
  },
  "peerDependencies": {
    "webpack": "^3.0.0"
  }
}
